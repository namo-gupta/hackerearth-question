#include<bits/stdc++.h>
using namespace std;
int main()
{
int t;
cin>>t;
while(t--)
{
long long n,r;
cin>>n>>r;
long long arr[n];
for(long long i=0;i<n;i++)
{
cin>>arr[i];
}
long long maxHeight=0;
long long count = 0;
for(long long i=0;i<n;i++)
{
if(arr[i]>maxHeight)
{
maxHeight = arr[i];
count++;
}
}
cout<<count * r<<"\n";
}
}
