#include<bits/stdc++.h>
using namespace std;
int main()
{
    ios_base::sync_with_stdio(false);
    cin.tie(NULL);
    
    long int n;
    cin>>n;
    long long int a=n*n;
    long long int arr[n-1];
    long long int s=0;
    for(long int i=0;i<n-1;i++)
    {
    cin>>arr[i];
    s+=arr[i];
    }
    cout<<a-s;
}
