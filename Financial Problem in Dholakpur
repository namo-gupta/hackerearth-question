#include<bits/stdc++.h>

using namespace std;

int main(){

int test;

cin>>test;

while(test--){

int n; cin>>n;

int arr[n];

for(int i = 0; i < n ; i++)

cin>>arr[i];

for(int i = 0 ; i < n; i++){

int count = 1, j = i -1;

while(j >= 0 && arr[j] <= arr[i]){

count++;

j--;

}

cout<<count<<" ";

}

cout<<"\n";

}

return 0;

}
